<Window x:Class="TorusWPF.ChangeFilterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TorusWPF"
        mc:Ignorable="d"
        Title="필터 바꾸기" Height="120" Width="800">
    <Grid>
        <TextBox x:Name="TextBoxValue" Margin="10,10,10,0" Text="" TextWrapping="NoWrap" VerticalAlignment="Top" KeyDown="TextBoxValue_KeyDown" />
        <Button x:Name="ButtonYes" Content="변경" HorizontalAlignment="Right" Margin="0,40,120,0" Width="100" VerticalAlignment="Top" Click="ButtonYes_Click"/>
        <Button x:Name="ButtonNo" Content="취소" HorizontalAlignment="Right" Margin="0,40,10,0" Width="100" VerticalAlignment="Top" Click="ButtonNo_Click" />
    </Grid>
</Window>
